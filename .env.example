# AWS Configuration
REACT_APP_AWS_REGION=ap-northeast-1
REACT_APP_AWS_USER_POOL_ID=your-user-pool-id
REACT_APP_AWS_USER_POOL_CLIENT_ID=your-client-id
REACT_APP_AWS_IDENTITY_POOL_ID=your-identity-pool-id
REACT_APP_API_GATEWAY_URL=https://your-api-gateway-url.execute-api.ap-northeast-1.amazonaws.com/dev
REACT_APP_WEBSOCKET_URL=wss://your-websocket-api-id.execute-api.ap-northeast-1.amazonaws.com/dev

# S3 Configuration
REACT_APP_S3_BUCKET_NAME=your-website-bucket-name
REACT_APP_FILES_BUCKET_NAME=your-files-bucket-name
REACT_APP_CLOUDFRONT_DOMAIN=your-cloudfront-domain.cloudfront.net

# Environment
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG_MODE=true

# Feature Flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=false